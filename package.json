{
  "name": "material-usage",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:dev": "yarn run clean && yarn run copy:images && NODE_ENV=development webpack --config webpack.config.js",
    "build:dist": "yarn run clean && yarn run copy:images && NODE_ENV=production webpack --config webpack.config.js",
    "build:watch": "yarn run clean && yarn run copy:images && NODE_ENV=development  webpack --config webpack.config.js -w",
    "clean": "rm -rf ./public",
    "copy:images": "mkdir -p ./public/images && cp -R ./client/images/* ./public/images",
    "start:dev": "source ./secrets.secrets && CONFIG=development nodemon ./www | bunyan -o short"
  },
  "dependencies": {
    "@babel/register": "7.0.0",
    "axios": "0.18.0",
    "bunyan": "1.8.12",
    "classnames": "2.2.6",
    "compression": "1.7.3",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "helmet": "3.14.0",
    "jss": "9.8.7",
    "libphonenumber-js": "1.6.4",
    "morgan": "1.9.1",
    "nconf": "0.10.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.1.0",
    "react-transition-group": "2.5.0",
    "redux": "4.0.1",
    "twilio": "3.23.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.2",
    "babel-loader": "8.0.4",
    "bootstrap": "4.1.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "mini-css-extract-plugin": "0.4.4",
    "node-sass": "4.9.4",
    "nodemon": "1.18.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.0.1",
